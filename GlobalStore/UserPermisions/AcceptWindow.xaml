<Window x:Class="GlobalStore.UserPermisions.AcceptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GlobalStore.UserPermisions"
        mc:Ignorable="d"
        Title="AcceptWindow" Height="450" Width="423.828" WindowStyle="None">
    <Grid Margin="0,0,417,0" Background="#242222">
        <StackPanel>
            <TextBox x:Name="adminPassword" HorizontalAlignment="Left" Height="22" Margin="97,184,-97,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="0"/>
            <Button x:Name="buttonAccept" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="150" Click="buttonAccept_Click"/>
            <Button x:Name="buttonCancel" Content="Button" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="150" Click="buttonCancel_Click"/>
        </StackPanel>
    </Grid>
</Window>
