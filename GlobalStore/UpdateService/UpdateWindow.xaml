<Window x:Class="GlobalStore.UpdateService.UpdateWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GlobalStore.UpdateService"
        mc:Ignorable="d"
        Title="UpdateWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button x:Name="cancelButton" Click="cancelButton_Click">
            </Button>

            <Button x:Name="acceptButton" Click="acceptButton_Click">
            </Button>
        </StackPanel>
    </Grid>
</Window>
